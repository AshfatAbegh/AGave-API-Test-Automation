{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "userId": {
      "type": "integer"
    },
    "fullName": {
      "type": "string"
    },
    "userProfilePictureUrl": {
      "type": "string"
    },
    "rating": {
      "type": "number"
    },
    "reviewCount": {
      "type": "integer"
    },
    "identified": {
      "type": "boolean"
    },
    "exhibitorArea": {
      "type": "string"
    },
    "treeSpecies": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "key": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        },
        "required": [
          "key",
          "value"
        ]
      }
    },
    "platformName": {
      "type": "object",
      "properties": {
        "Instagram": {
          "type": "object",
          "properties": {
            "Name": {
              "type": "string"
            },
            "Link": {
              "type": "string"
            }
          },
          "required": [
            "Name",
            "Link"
          ]
        }
      }
    },
    "selfIntroductoryStatement": {
      "type": "string"
    }
  },
  "required": [
    "userId",
    "fullName",
    "userProfilePictureUrl",
    "rating",
    "reviewCount",
    "identified",
    "exhibitorArea",
    "treeSpecies",
    "platformName",
    "selfIntroductoryStatement"
  ]
}