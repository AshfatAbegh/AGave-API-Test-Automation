      "type": "object",
      "properties": {
        "shippingAddressPostalCode": {
          "type": "string"
        },
        "shippingAddressPrefectures": {
          "type": "string"
        },
        "shippingAddressCityStreetAddress": {
          "type": "string"
        },
        "shippingAddressBuildingNameAndRoomNumber": {
          "type": "string"
        }
